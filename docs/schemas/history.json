{
  "type": "object",
  "required": ["history_json_contract_version", "runs"],
  "properties": {
    "history_json_contract_version": { "type": "integer" },
    "runs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["timestamp", "overall", "exit_code", "hosts"],
        "properties": {
          "timestamp": { "type": "string" },
          "timestamp_epoch": { "type": ["integer", "null"] },
          "overall": { "type": "string" },
          "exit_code": { "type": "integer" },
          "logfile": { "type": ["string", "null"] },
          "summary_file": { "type": ["string", "null"] },
          "summary_json": { "type": ["string", "null"] },
          "hosts": {
            "type": "object",
            "required": ["ok", "warn", "crit", "unknown", "skipped"],
            "properties": {
              "ok": { "type": "integer" },
              "warn": { "type": "integer" },
              "crit": { "type": "integer" },
              "unknown": { "type": "integer" },
              "skipped": { "type": "integer" }
            }
          },
          "top_reasons": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["reason", "count"],
              "properties": {
                "reason": { "type": "string" },
                "count": { "type": "integer" }
              }
            }
          }
        }
      }
    }
  }
}
